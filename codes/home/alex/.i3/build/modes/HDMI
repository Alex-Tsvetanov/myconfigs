#HDMI mode
mode "HDMI"
{
	bindsym $mod + 1 workspace "$workspace1 - HDMI"
	bindsym $mod + 2 workspace "$workspace2 - HDMI"
	bindsym $mod + 3 workspace "$workspace3 - HDMI"
	bindsym $mod + 4 workspace "$workspace4 - HDMI"
	bindsym $mod + 5 workspace "$workspace5 - HDMI"
	bindsym $mod + 6 workspace "$workspace6 - HDMI"
	bindsym $mod + 7 workspace "$workspace7 - HDMI"
	bindsym $mod + 8 workspace "$workspace8 - HDMI"
	bindsym $mod + 9 workspace "$workspace9 - HDMI"
	bindsym $mod + 0 workspace "$workspace10 - HDMI"
	
	bindsym $mod + Shift+1 move container to workspace "$workspace1 - HDMI"
	bindsym $mod + Shift+2 move container to workspace "$workspace2 - HDMI"
	bindsym $mod + Shift+3 move container to workspace "$workspace3 - HDMI"
	bindsym $mod + Shift+4 move container to workspace "$workspace4 - HDMI"
	bindsym $mod + Shift+5 move container to workspace "$workspace5 - HDMI"
	bindsym $mod + Shift+6 move container to workspace "$workspace6 - HDMI"
	bindsym $mod + Shift+7 move container to workspace "$workspace7 - HDMI"
	bindsym $mod + Shift+8 move container to workspace "$workspace8 - HDMI"
	bindsym $mod + Shift+9 move container to workspace "$workspace9 - HDMI"
	bindsym $mod + Shift+0 move container to workspace "$workspace10 - HDMI"

	bindsym $mod + Escape mode "Display"

	#include "../bindsym"
	#include "../rebuild"
}
